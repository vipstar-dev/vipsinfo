{
  "name": "vipsinfo",
  "version": "0.1.6",
  "description": "vipsinfo core node and libraries",
  "scripts": {
    "dev": "yarn build:live",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf dist",
    "compile": "tsc -p . && tsc-alias -p tsconfig.json && cp -r types dist/",
    "build:live": "ts-node-dev --respawn -r tsconfig-paths/register bin/vipsinfo-node.ts start",
    "auto-publish": "yarn build && cd dist && npm publish"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vipstar-dev/vipsinfo.git"
  },
  "keywords": [
    "VIPSTARCOIN",
    "VIPSTARCOIN info",
    "VIPSTARCOIN explorer"
  ],
  "author": "y-chan <y-chan@y-chan.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vipstar-dev/vipsinfo/issues"
  },
  "homepage": "https://github.com/vipstar-dev/vipsinfo#readme",
  "files": [
    "lib",
    "node",
    "p2p",
    "rpc",
    "types"
  ],
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/ethereumjs-abi": "^0.6.3",
    "@types/liftoff": "^2.5.0",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^14.14.32",
    "@types/socket.io": "^2.1.13",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsc-alias": "^1.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "colors": "^1.3.3",
    "commander": "^3.0.1",
    "ethereumjs-abi": "^0.6.8",
    "liftoff": "^3.1.0",
    "lru-cache": "^5.1.1",
    "mysql2": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "secp256k1": "^3.7.1",
    "sequelize": "^6.5.1",
    "sequelize-typescript": "^2.1.0",
    "socket.io": "^2.3.0",
    "uuid": "^3.3.3"
  }
}
